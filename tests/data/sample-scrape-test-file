    function subMitEmailForm(rid){ 
        $(".buttondiv").html('<img src="https://www.filepicker.io/static/img/spinner.gif"/>');
        var url = "http://chinese.food.com/services/mobile/email/recipe?rid=128611",
            data = {
                fromemail: $("#fromemail").val(),
                toemail: $("#toemail").val(),
                fromname: $("#fromname").val(),
                msgtext: $("#msgtext").val(),
                shorturl: shortUrl,
                recaptcha_challenge_field: $("#recaptcha_challenge_field").val(),
                recaptcha_response_field: $("#recaptcha_response_field").val()
            };

         $.ajax({
             type: "POST",
             url: url, //give url here
             data: data,
             dataType: 'json',
             success: function(data){
                     var responseType = data.responseType;
                     if(responseType == 'SUCCESS'){
                             $( ".success-message" ).html( "Your message was successfully sent." );
                     }else{
                             $( ".success-message" ).html( "Error Occured. Please try again!" );
                     }
                     Recaptcha.reload();
                     $(".buttondiv").html('<button id="submit-req" class="btn vip" onclick="subMitEmailForm(128611)">Send</button>');
             }
         });

    }
</script>











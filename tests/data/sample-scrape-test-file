    // initialize components of the toolbar.
    function initializeToolbar() {
        $('#email-modal .icon-caret-left').css("left", $(".header").offset().left + 2);
    }
    $(document).ready(function () {
        initializeToolbar();
        $('#email-modal .icon-caret-left').css("left", $(".header").offset().left + 2);
        $(window).resize(function () {
            initializeToolbar();
        });
        var emailContent = $('#emailDialog');
        var emailHandle = $(".emailRecipe");
        var emailDialog = fdc.ui.dialog({
                content: emailContent,
                handle: emailHandle,
                dialogOptions: {
                    dialogClass: 'fdc-allow-overflow',
                    buttons: 'none',
                    title: 'Email This',
                    height: 580,
                    width: 340,
                    position: {
                        my: "left top",
                        at: "right top",
                        of: emailContent.parent()
                    }
                },
                authentication: false
            });


        // TODO: add comments for this code block
        alignSocialBar($(this).scrollTop());
        $(window).scroll(function () {
            alignSocialBar($(this).scrollTop());
        });


        // TODO: explain what this does here
        function alignSocialBar(scrolledPix) {
            var topPix = $(".tab-nav").offset().top + 35 - scrolledPix,
                $toolbar = $(".recipe-toolbar"),
                $emailModalPointer = $('#email-modal .icon-caret-left'),
                tbTopPosition = (topPix > 0) ? topPix + 'px' : 0,
                empTopPosition = (topPix > 0) ? (topPix + 150) + 'px' : '0px';

            //$toolbar.css("top", tbTopPosition);
            $emailModalPointer.css("top", empTopPosition);
        }

    });
    var RecaptchaOptions = {
        theme : 'white'
    };


    // notes.js initialized on doc ready
    SNI.RZ.Recipe.NotesConfig = {
        recipeId: "128611",
        privateNotesId: "0",
        noteMemberId: "0"
    };

    // initialize recipe page components
    (function($){

        SNI.RZ.Recipe.init({
            title: 'Chinese Barbecue Sauce',
            id: '128611'
        });


        // Meal Planner integration system variables
        MP = {};
        MP.env = 'http://www.food.com';
        MP.imageServerUrl = 'http://img.food.com/';
        MP.shareServerLogin = 'http://share.food.com/registration/login.esi?DEST_URL=';
        MP.calendarPlanDays = {};

        MP.recipeId = "128611";
        MP.recipeTitle = "Chinese Barbecue Sauce";

        $("a.removeNotes").click(function () {
            var $url="/dpn.do?pid=0&mid=0&rid=128611";
            $.ajax({
                url: $url,
                success: function(){document.location = document.location+"";}
            });
            return false;
        });

        // Hide the next recipe module if the open HTML module is displayed
        var rrOpenHtml = $("div.rr-open-html");

        // If right rail open html is present...then hide next recipe
        if(rrOpenHtml.html() != undefined && rrOpenHtml.html() != "") {
            $("div.next-recipe").hide();
        }

        Modernizr.load([
            'http://img.food.com/js/fdc-planner-recipe.min.js',
            'http://img.food.com/js/fdc-photobox.min.js',
        ]);

    })(jQuery);


    SNI.RZ.CustomDropdown.addToRecipeLinks("128611");


        // initialize all social media widgets
        socBar = new fdc.Socialbar({

{
    "name": "Chinese food recipes",
    "downloadImages": false,
    "maxDepth" : 2,
    "timeout" : 3000,
    "interval" : 250,
    "cache" : false,
    "output" : {
        "format" : "json",
        "directory" : "output"
    },
    "start" : [
        "http://localhost:8181/tests/data/chinese-food-list"
    ],
    "crawl": [
        "/all/chinese-food?pn=([0-9]+)",
        "/tests/data/([0-9A-Za-z-_]+)"
    ],
    "extract": [
        {
            "name": "recipe",
            "description": "Recipe",
            "match": "body",
            "children": [
                {
                    "name": "ingredients",
                    "description": "Ingredients",
                    "match": ".ingredients",
                    "type" : "array",
                    "children": [
                        {
                            "name": "ingredient",
                            "description": "Ingredient",
                            "match": "li.ingredient",
                            "children": [
                                {
                                    "name": "value",
                                    "description": "Value",
                                    "match": ".value"
                                },
                                {
                                    "name": "type",
                                    "description": "Type",
                                    "match": ".type"
                                },
                                {
                                    "name": "name",
                                    "description": "Name",
                                    "match": ".name"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}